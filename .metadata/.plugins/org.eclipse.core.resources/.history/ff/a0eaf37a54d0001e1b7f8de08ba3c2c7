package org.kh.site2;

public class Operation7 {
	public static void main(String[] args) {
		int a = 20;
		int b = 5;
		int temp;
		
		
		int x = 0b01101;
		int y = 0b01011;
		
		System.out.println(Integer.toBinaryString(x));
		System.out.println(Integer.toBinaryString(y));
		System.out.println(a |= b);
		System.out.println(a &= b);
		System.out.println(a >>= b);
		System.out.println(a <<= b);
		System.out.println(a ^= b);
	}
}
