package org.kh.java;

public class Variable8 {
	public static void main(String[] args) {
		var num1 = 29.54;
	    var num2 = 34;
	    var num3 = 'k';
	    var num4 = "";
	    
	    num1 = (char) 't';
	    
	    
	    var add = num1 + num3;
	    
	    System.out.println((int)num1);
	    System.out.println((int) num3);
	    System.out.println(add);
	}
}
